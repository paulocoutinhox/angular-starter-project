{
	"name": "angular-starter-project",
	"version": "2.0.0",
	"description": "Angular Starter Project",
	"main": "index.js",
	"scripts": {
		"clean": "rm -rf aot && rm -rf dist && find ./ -type f -name '.DS_Store' -exec rm {} + && find ./src/app/ -type f -name '*.metadata.json' -exec rm {} + && find ./src/app/ -type f -name '*.ngfactory.ts' -exec rm {} + && find ./src/app/ -type f -name '*.css.shim.ts' -exec rm {} + && find ./src/app/ -type f -name '*.js' -exec rm {} + && find ./src/app/ -type f -name '*.js.map' -exec rm {} +",
		"explorer": "source-map-explorer ./dist/bootstrap.bundle.js ./dist/bootstrap.bundle.js.map",
		"ngc": "ngc -p ./tsconfig.json",
		"serve": "lite-server -c ./bs-config.js",
		"start": "npm run ngc && concurrently -r \"webpack-dev-server --config webpack.config.js\" \"node ./bin/ngc-watch.js\"",
		"start-prod": "npm run build && npm run serve",
		"build": "npm run ngc && npm run webpack-prod",
		"webpack": "rm -rf ./dist && webpack --config webpack.config.js",
		"webpack-prod": "rm -rf ./dist && webpack --config webpack.prod.config.js",
		"tests-e2e": "protractor tests/e2e/protractor.conf.js"
	},
	"keywords": [
		"angular2",
		"aot",
		"typescript",
		"webpack"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/prsolucoes/angular2-starter-project.git"
	},
	"author": {
		"name": "Paulo Coutinho",
		"email": "paulo@prsolucoes.com"
	},
	"license": "MIT",
	"dependencies": {
		"@angular/common": "2.4.9",
		"@angular/compiler": "2.4.9",
		"@angular/core": "2.4.9",
		"@angular/forms": "2.4.9",
		"@angular/http": "2.4.9",
		"@angular/platform-browser": "2.4.9",
		"@angular/platform-browser-dynamic": "2.4.9",
		"@angular/upgrade": "2.4.9",
		"@angular/router": "3.4.9",
		"bootstrap": "3.3.7",
		"core-js": "2.4.1",
		"rxjs": "5.2.0",
		"zone.js": "0.7.7",
		"jquery": "3.1.1"
	},
	"devDependencies": {
		"@angular/compiler-cli": "2.4.9",
		"@angular/platform-server": "2.4.9",
		"@ngtools/webpack": "1.2.11",
		"@types/core-js": "0.9.35",
		"@types/node": "7.0.5",
		"@types/jquery": "2.0.40",
		"copy-webpack-plugin": "4.0.1",
		"css-loader": "0.26.2",
		"extract-text-webpack-plugin": "2.0.0",
		"file-loader": "0.10.1",
		"lite-server": "2.2.2",
		"raw-loader": "0.5.1",
		"html-loader": "0.4.5",
		"source-map-explorer": "1.3.3",
		"style-loader": "0.13.2",
		"to-string-loader": "1.1.5",
		"typescript": "2.2.1",
		"url-loader": "0.5.8",
		"webpack": "2.2.1",
		"webpack-dev-server": "2.4.1",
		"concurrently": "3.4.0",
		"chokidar": "1.6.1",
		"http-proxy-middleware": "0.17.3",
		"connect-history-api-fallback": "1.3.0",
		"protractor": "5.1.1"
	}
}